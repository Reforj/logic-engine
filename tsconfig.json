{
  "compilerOptions": {
    "target":"ESNext",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "declaration":false,
    "noEmit": true,
    "skipLibCheck": true,
    "emitDecoratorMetadata":false,
    "experimentalDecorators":true,
    "module":"commonjs",
    "moduleResolution":"node",
    "sourceMap":true,
    "jsx":"react-jsx",
    "allowJs": true,
    "resolveJsonModule":true,
    "allowSyntheticDefaultImports":true,
    "esModuleInterop": true ,
    "noImplicitAny":false,
    "noImplicitThis":true,
    "noImplicitReturns":true,
    "strictNullChecks":true,
    "useUnknownInCatchVariables": false,
    "baseUrl": ".",
    "paths": {
      "*": [
        "*",
      ],
      "~/*": ["*"]
    },
    "typeRoots": [
      "./public",
      "./node_modules/@types"
    ]
  },
  "exclude": [
    "**/*.spec.ts",
    "**/*.test.ts",
    "node_modules",
  ],
  "include": [
    "./",
  ],
  "compileOnSave":false
}
